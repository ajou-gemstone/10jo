<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.capstonedesignandroid">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-feature android:name="android.hardware.camera"
        android:required="true" />


    <application
        android:icon="@mipmap/ic_launcher"
        android:label="ASMR"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LectureroomCheckPictureActivity"></activity>
        <activity android:name=".LectureroomCheckActivity" />
        <activity
            android:name=".testActivity"
            android:label="@string/title_activity_test"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".CafeMapActivity" />
        <activity
            android:name=".LectureroomReservationActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".StudyBulletinBoardActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".MakeGroupActivity" />
        <activity android:name=".ReadGroupActivity" />
        <activity android:name=".TabFragment1" />
        <activity android:name=".TabFragment2" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme"
            tools:ignore="WrongManifestParent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CafeDetailedInfoActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <meta-data
            android:name="com.google.android.gms.ads.AD_MANAGER_APP"
            android:value="true"/>
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="fa48347431d94978a0df221d44686add" />
        <provider
            android:name="androidx.core.content.FileProvider"
            android:authorities="com.example.capstonedesignandroid"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths"></meta-data>
        </provider>
    </application>

</manifest>